<?php 
namespace Instasaver\IndexEnBundle\Entity;
use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="users")
 */
class Register {
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
	protected $id;
    /**
     * @ORM\Column(type="string", length=255)
     */
	protected $email;
    /**
     * @ORM\Column(type="string", length=255)
     */
	protected $username;
    /**
     * @ORM\Column(type="string", length=255)
     */
	protected $password;
}
